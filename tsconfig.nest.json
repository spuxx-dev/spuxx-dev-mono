{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "es2022",
    "moduleResolution": "Node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "declaration": true,
    "incremental": true,
    "strictNullChecks": false,
    "strictBindCallApply": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
